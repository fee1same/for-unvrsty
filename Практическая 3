a = float(input('Ввведите 1 число: '))
type(a)
b = float(input('Введите 2 число: '))
type(b)
c = float(input('Введите 3 число: '))
type(c)
z = 1
x = 3
if (a>=z and a<=x):
	print('Число', a, 'соответствует промежутку')
if (b>=z and b<=x):
	print('Число', b, 'соответствует промежутку')
if (c>=z and c<=x):
	print('Число', c, 'соответствует промежутку')
