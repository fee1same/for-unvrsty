def F(n):
    print(a*b)
def X(n):
    print(c*d)
def C(n):
    print(e*n)
print("Прямоугольник 1, первая сторона: ")
a=int(input())
print("Прямоугольник 1, вторая сторона: ")
b=int(input())
print("Прямоугольник 2, первая сторона: ")
c=int(input())
print("Прямоугольник 2, вторая сторона: ")
d=int(input())
print("Прямоугольник 3, первая сторона: ")
e=int(input())
print("Прямоугольник 3, вторая сторона: ")
n=int(input())
print("Площадь прямоугольника 1: ")
print(F(n))
print("Площадь прямоугольника 2: ")
print(X(n))
print("Площадь прямоугольника 3: ")
print(C(n))
